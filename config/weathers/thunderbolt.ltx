[thunderbolt_common]
altitude				= 10, 80 			;altitude in deg (min/max по вертикали)
delta_longitude			= 360				;in deg 	(разброс по горизонту)
min_dist_factor			= 0.8 ;0.94				;(<0.95) 	(расстояние от FAR_PLANE)
tilt					= 17				;lightning tilt in deg (15-30) (отклонение молнии от оси)
second_propability		= 0.2				;(0-1) 		(вероятность появления следующей молнии после этой)
sky_color				= 0.1				;(0-1) 		(коэффициет сколько прибавится к цвету неба)
sun_color				= 0.9				;(0-1) 		(коэффициет сколько прибавится к цвету солнца)
fog_color				= 0.1				;(0-1) 		(коэффициет сколько прибавится к цвету fog)

