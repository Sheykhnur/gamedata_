[torch_definition]
range_max       = 35									; range max (skyloader: when battery is full)                 <in meters>
range_max_r2    = 30									; range max                <in meters>
range_curve     = 10 									; range curvature, the formula is: range = range_max * atan(range_curve * condition) / (pi/2)
range_min       = 8 ; DELETE
range_min_r2    = 8 ; DELETE
color           = 0.95,1.05,1.6,0.61						; four color components <r,g,b,a> 0.f<X<3.f
color_r2        = 1.05,0.9,1.6,0.61						; four color components <r,g,b,a> 0.f<X<3.f
omni_range      = 1.15									; range                 <in meters>
omni_range_r2   = 0.0									; range                 <in meters>
omni_color      = 1.0,0.9,0.9,0.3						; four color components <r,g,b,a> 0.f<X<3.f
omni_color_r2   = 1.0,1.0,1.0,0.2						; four color components <r,g,b,a> 0.f<X<3.f
color_animator  = headlamp								; color animator name   (empty - not using animator)
spot_texture    = lights\lights_bspot01					; spot texture name     (using only in Second Render)
spot_angle      = 65									; spot angle            <in gradus>
glow_texture    = glow\glow_torch_r2					; glow texture name     (required)
glow_radius     = 0.1									; glow radius           <in meters>
guide_bone      = cone_bone								; guid bone name        (required)
