;HOSPITAL CELL
[logic]
active = ph_door@locked

[ph_door@locked]
closed = true
locked = true
snd_open_start = cell_locked
on_info = {+dam_guide_spawn} ph_door@open

[ph_door@open]
closed = false
locked = false
snd_open_start = cell_open
snd_close_start = metal_small_close_start
snd_close_stop = trader_door_close_stop
on_use = ph_door@closed

[ph_door@closed]
closed = true
locked = false
snd_open_start = cell_open
snd_close_start = metal_small_close_start
snd_close_stop = trader_door_close_stop
on_use = ph_door@open