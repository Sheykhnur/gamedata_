;GATE TO SECRET LAB
[logic]
active = ph_gate@locked

[ph_gate@locked]
state = closed
locking = hard
on_info = {=actor_has_secret_key} ph_gate@closed
;on_use = ph_gate@unlocked

[ph_gate@closed]
state = closed
locking = none
on_use = ph_gate@open

[ph_gate@open]
state = open
locking = none
on_info = {-ros_key_taken} %+ros_key_taken =take_ros_key%
on_use = ph_gate@closed