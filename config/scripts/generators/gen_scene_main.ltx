[logic]
active = sr_idle

[sr_idle]
on_actor_inside = {+gen_ghost_instructions_recieved} %=disable_ui =run_postprocess(blink) =play_snd(affects\hit_fist) =play_snd(heart\heart) =tasks_generators.gen_set_pos% sr_idle@snd, nil %=kill_actor%

[sr_idle@snd]
on_timer = 500 | %=play_snd(characters_voice\scenario\gen\gen_psi)% sr_idle@2

[sr_idle@2]
on_timer = 14000 | %=play_snd(actor\breath_1) =run_postprocess(dark_fade)% sr_idle@snd2

[sr_idle@snd2]
on_timer = 500 | %=play_snd(characters_voice\scenario\gen\gen_fall)% sr_idle@21

[sr_idle@21]
on_timer = 500 | %+gen_scene_fade% 
on_timer2 = 800 | sr_idle@3

[sr_idle@3]
on_timer = 550 | %=teleport_actor(gen_player_teleport:gen_player_teleport_look) =run_cam_effector(gen_scene_main2:2798:false) =run_postprocess(gen_wake)% sr_idle@4

[sr_idle@4]
on_timer = 12200 | %=play_snd(characters_voice\scenario\gen\gen_dram_moment) =play_snd(characters_voice\scenario\gen\gen_gul)% sr_idle@5

[sr_idle@5]
on_timer = 1000 | %=play_snd(ambient\earthquake)% sr_idle@6

[sr_idle@6]
on_timer = 9000 | %=enable_ui% sr_idle@7

[sr_idle@7]
on_timer = 2000 | %+gen_chimera_activate% nil