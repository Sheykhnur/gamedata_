[logic]
active = heli_move@start

[heli_move@start]
path_move = ds_heli_starter
engine_sound = false
combat_safe_altitude = -10
combat_use_rocket = false
combat_use_mgun = false
default_velocity = 60
on_info = {-ds_sin_blackout +ds_heli_start} heli_move@1


[heli_move@1]
path_move = ds_heli_zone1
combat_velocity = 20
combat_safe_altitude = -1
combat_enemy = actor
combat_use_rocket = false
combat_use_mgun = true
;invulnerable = true
on_info = {+ds_heli_stop} heli_move@start
default_velocity = 40
