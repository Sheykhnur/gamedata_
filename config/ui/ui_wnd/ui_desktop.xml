<?xml version="1.0" encoding="windows-1251" ?>

<window>

	<!-- COMMON CONTROLS -->
	<button_ordinary x="0" y="0" width="117" height="29">
		<texture>ui_button_ordinary</texture>
	</button_ordinary>

	<!-- DESKTOP CONTROLS -->
	<desktop>
		<monitor_frame x="0" y="0" width="1024" height="768">
			<texture x="0" y="0" width="1024" height="768">ui\pc\monitor_frame</texture>
		</monitor_frame>
		<wallpaper_pc_locked x="0" y="0" width="1024" height="768">
			<texture>ui\pc\blackscreen</texture>
		</wallpaper_pc_locked>
		<wallpaper1 x="0" y="40" width="1024" height="768">
			<texture x="0" y="1" width="1024" height="680">ui\pc\wallpapers\ui_pc_wallpaper1</texture>
		</wallpaper1>
		<wallpaper2 x="0" y="40" width="1024" height="768">
			<texture x="0" y="0" width="1024" height="680">ui\pc\wallpapers\ui_pc_wallpaper2</texture>
		</wallpaper2>
		<wallpaper3 x="0" y="40" width="1024" height="768">
			<texture x="0" y="0" width="1024" height="680">ui\pc\wallpapers\ui_pc_wallpaper3</texture>
		</wallpaper3>
		<wallpaper4 x="0" y="40" width="1024" height="768">
			<texture x="0" y="0" width="1024" height="680">ui\pc\wallpapers\ui_pc_wallpaper4</texture>
		</wallpaper4>
		<wallpaper5 x="0" y="40" width="1024" height="768">
			<texture x="0" y="0" width="1024" height="680">ui\pc\wallpapers\ui_pc_wallpaper5</texture>
		</wallpaper5>
		<wallpaper6 x="0" y="40" width="1024" height="768">
			<texture x="0" y="0" width="1024" height="680">ui\pc\wallpapers\ui_pc_wallpaper6</texture>
		</wallpaper6>
		<wallpaper7 x="0" y="40" width="1024" height="768">
			<texture x="0" y="0" width="1024" height="680">ui\pc\wallpapers\ui_pc_wallpaper7</texture>
		</wallpaper7>
		<wallpaper_iaea x="0" y="40" width="1024" height="768">
			<texture x="0" y="0" width="1024" height="680">ui\pc\wallpapers\ui_pc_wallpaper_iaea</texture>
		</wallpaper_iaea>
		<wallpaper_water x="0" y="40" width="1024" height="768">
			<texture x="0" y="0" width="1024" height="680">ui\pc\wallpapers\ui_pc_wallpaper_water</texture>
		</wallpaper_water>
		<wallpaper_dso x="0" y="40" width="1024" height="768">
			<texture x="0" y="1" width="1024" height="680">ui\pc\wallpapers\ui_pc_wallpaper_dso</texture>
		</wallpaper_dso>
		<stc_timer x="50" y="730" width="1024" height="32">
			<texture x="0" y="0" width="95" height="32">ui\ui_old_textures</texture>
			<text x="25" y="5" font="graffiti22" r="207" g="155" b="80" />
		</stc_timer>
		<stc_caption x="0" y="5" width="1024" height="32">
			<text font="graffiti32" r="207" g="155" b="80" align="c">ui_pc_static_smart_os</text>
		</stc_caption>
		<stc_xray_tech x="0" y="730" width="1024" height="32">
			<text font="graffiti32" r="155" g="148" b="148" align="c">ui_pc_static_xray_tech</text>
		</stc_xray_tech>
		<stc_cerberus x="380" y="727" width="256" height="36">
			<texture x="0" y="0" width="256" height="37">ui\pc\cerberus</texture>
		</stc_cerberus>
		<power_btn x="920" y="723" width="40" height="40" stretch="1">
			<texture>ui_pc_icon_power_off_big</texture>
		</power_btn>
		<monitor_led x="900" y="735" width="16" height="18">
			<texture x="122" y="240" width="16" height="18">ui\ui_common</texture>
		</monitor_led>
	</desktop>
	
	<!-- TEXT VIEWER CONTROLS -->
	<text_viewer>
		<string_list x="0" y="0" width="414" height="290" item_height="18" always_show_scroll="1">
			<font font="letterica16" r="238" g="155" b="23" />
		</string_list>
		<horizontal_line x="0" y="0" width="420" height="4" stretch="1">
			<texture>ui\ui_pda_horizontal_line</texture>
		</horizontal_line>
	</text_viewer>
	
	<!-- GALLERY VIEWER CONTROLS -->	
	<image_viewer>
		<listview x="5" y="5" width="190" height="200" item_height="18" always_show_scroll="1">
			<font font="letterica16" r="216" g="186" b="140" />
		</listview>
		<btn_prev x="0" y="0" width="44" height="29">
			<texture_e>ui_button_arrow_left_e</texture_e>
			<texture_h>ui_button_arrow_left_h</texture_h>
			<texture_t>ui_button_arrow_left_t</texture_t>
			<texture_d>ui_button_arrow_left_d</texture_d>
		</btn_prev>
		<btn_next x="0" y="0" width="44" height="29">
			<texture_e>ui_button_arrow_right_e</texture_e>
			<texture_h>ui_button_arrow_right_h</texture_h>
			<texture_t>ui_button_arrow_right_t</texture_t>
			<texture_d>ui_button_arrow_right_d</texture_d>
		</btn_next>
	</image_viewer>
	
	<!-- PASSWORD EDIT BOX CONTROLS -->	
	<passwd_editbox>
		<edit_box_passwd x="294" y="91" width="250" height="24" password="1" max_symb_count="26">
			<text font="letterica18" r="207" g="155" b="80" />
			<texture>ui_linetext_e</texture>
		</edit_box_passwd>
		<edit_box_letters x="294" y="91" width="250" height="24" password="0" max_symb_count="26">
			<text font="letterica18" r="207" g="155" b="80" />
			<texture>ui_linetext_e</texture>
		</edit_box_letters>
		<stc_wrong_passwd x="294" y="91" width="250" height="24" light_anim="ui_slow_blinking_alpha" la_cyclic="1" la_texture="0" la_text="1" la_alpha="1">
			<text x="0" y="0" font="letterica16" r="240" g="217" b="182" a="255" align="c">ui_pc_static_wrong_password</text>
		</stc_wrong_passwd>
	</passwd_editbox>
	
	<!-- MEDIA PLAYER CONTROLS -->
	<media_player>
		<duration_slot x="0" y="0" width="284" height="16">
			<texture x="0" y="291" width="284" height="16">ui\pc\ui_mplayer_skin</texture>
		</duration_slot>
		<duration_bar x="0" y="0" width="264" height="10" horz="1" min="0" max="100" pos="0">
			<progress>
				<texture>ui\pc\ui_mplayer_duration_pbar</texture>
			</progress>
		</duration_bar>
	</media_player>
	
	<!-- HEX VIEWER CONTROLS -->
	<hex_viewer>
		<string_list x="0" y="0" width="484" height="325" item_height="18" always_show_scroll="0">
			<font font="letterica16" r="238" g="155" b="23" />
		</string_list>
	</hex_viewer>
	
	<!-- CODE INSPECTOR CONTROLS -->
	<code_viewer>
		<string_list x="0" y="0" width="565" height="405" item_height="18" always_show_scroll="0">
			<font font="letterica16" r="238" g="155" b="23" />
		</string_list>
	</code_viewer>
	
	<!-- CONSOLE CONTROLS -->
	<console_viewer>
		<console_list x="0" y="0" width="485" height="360" item_height="16" always_show_scroll="1">
			<font font="letterica16" r="238" g="155" b="23" />
		</console_list>
	</console_viewer>

</window>
