			<texture>ui\pc\ui_mplayer_spectrum_pbar</texture>
		</progress>
	</spectrum_bar>
	<duration_bar x="482" y="343" width="264" height="10" horz="1" min="0" max="100" pos="0">
		<progress>
			<texture>ui\pc\ui_mplayer_duration_pbar</texture>
		</progress>
	</duration_bar>
	
	<volume_holder x="564" y="370" width="97" height="49">
