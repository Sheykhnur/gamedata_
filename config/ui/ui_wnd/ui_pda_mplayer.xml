	</right_frame_header>
	
	<anim_static x="721" y="14" width="32" height="32" frames="16" duration="1000" columns="8" frame_width="32" frame_height="32" cyclic="1" autoplay="1">
		<texture>ui\ui_pda_anims_04</texture>
	</anim_static>
	
	<visualisation_skin x="471" y="50" width="284" height="307">
		<texture>ui\pc\ui_mplayer_skin</texture>
	</visualisation_skin>
