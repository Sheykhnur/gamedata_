	<!-- playlist -->
	<left_frame_window x="0" y="0" width="448" height="295">
		<texture>ui_pda_frame_sub</texture>
		<auto_static x="28" y="40" width="20" height="20">
			<text align="r" font="letterica16" r="238" g="153" b="26" a="255">##</text>
		</auto_static>
		<auto_static x="58" y="40" width="145" height="20">
			<text align="l" font="letterica16" r="238" g="153" b="26" a="255">ui_pda_mplayer_title</text>
		</auto_static>
		<auto_static x="213" y="40" width="145" height="20">
			<text align="l" font="letterica16" r="238" g="153" b="26" a="255">ui_pda_mplayer_artist</text>
		</auto_static>
		<auto_static x="368" y="40" width="55" height="20">
			<text align="l" font="letterica16" r="238" g="153" b="26" a="255">ui_pda_mplayer_duration</text>
		</auto_static>
	</left_frame_window>
	<left_frame_header x="8" y="17" width="434" height="21">
		<texture>ui_string_03</texture>
		<title x="10" y="1" width="434" height="18">
			<text font="letterica16" r="255" g="255" b="255" complex_mode="0">ui_pda_mplayer_caption</text>
		</title>
