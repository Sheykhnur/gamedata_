	<playlist_lines_header>
		<auto_static x="53" y="40" width="1" height="20">
			<texture>ui\pc\ui_hor_line_list</texture>
		</auto_static>
		<auto_static x="208" y="40" width="1" height="20">
			<texture>ui\pc\ui_hor_line_list</texture>
		</auto_static>
		<auto_static x="363" y="40" width="1" height="20">
			<texture>ui\pc\ui_hor_line_list</texture>
		</auto_static>
	</playlist_lines_header>
	
	<playlist x="8" y="58" width="434" height="234" item_height="18" always_show_scroll="1" can_select="1" scroll_profile="pda" />
	
	<!-- options (left bottom frame) -->
	<options_frame_window x="0" y="300" width="448" height="153">
		<texture>ui_pda_frame_sub</texture>
		<auto_static x="10" y="45" width="142" height="18">
			<text font="letterica16" r="255" g="255" b="255">ui_pda_mplayer_playback_options</text>
		</auto_static>
		<auto_static x="210" y="45" width="142" height="18">
			<text font="letterica16" r="255" g="255" b="255">ui_pda_mplayer_notify</text>
		</auto_static>
	</options_frame_window>
	<options_frame_header x="8" y="317" width="434" height="21">
		<texture>ui_string_03</texture>
		<title x="10" y="1" width="434" height="18">
			<text font="letterica16" r="255" g="255" b="255" complex_mode="0">Options</text>
		</title>
	</options_frame_header>
	
	<tab_playback x="0" y="365" width="142" height="80" radio="1">
		<button x="0" y="00" width="142">
			<text font="letterica16" r="115" g="114" b="112">ui_pda_mplayer_none</text>
		</button>
		<button x="0" y="20" width="142">
			<text font="letterica16" r="115" g="114" b="112">ui_pda_mplayer_random</text>
		</button>
		<button x="0" y="40" width="142">
			<text font="letterica16" r="115" g="114" b="112">ui_pda_mplayer_repeat_playlist</text>
		</button>
		<button x="0" y="60" width="142">
			<text font="letterica16" r="115" g="114" b="112">ui_pda_mplayer_repeat_track</text>
		</button>
	</tab_playback>
	
	<btn_notify x="210" y="365" width="142" height="21">
		<text font="letterica16" r="215" g="195" b="170" complex_mode="0">ui_pda_mplayer_off</text>
	</btn_notify>
	
	<!-- now_playing_frame -->
	<right_frame_window x="463" y="0" width="298" height="453">
		<texture>ui_pda_frame_sub</texture>
	</right_frame_window>
	
	<right_frame_header x="471" y="16" width="284" height="27">
