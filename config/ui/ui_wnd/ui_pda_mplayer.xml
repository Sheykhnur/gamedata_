			<texture>ui\pc\ui_mplayer_volume_1</texture><!-- 96x48 -->
		</progress>
	</volume_bar>
	
	<volume_stc x="583" y="425" width="58" height="20">
		<text align="c" font="letterica16" r="238" g="153" b="26" a="255" />
	</volume_stc>
	
	<btn_volume_down x="561" y="420" width="24" height="25">
		<texture>ui_beltbut_min</texture>
	</btn_volume_down>
	<btn_volume_up x="641" y="420" width="24" height="25">
		<texture>ui_beltbut_plus</texture>
	</btn_volume_up>
	
</w>
