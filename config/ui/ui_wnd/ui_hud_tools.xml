<?xml version="1.0" encoding="windows-1251" ?>

<window>
	
	<wnd_frame x="0" y="0" width="600" height="400">
		<texture>ui_stroketextbox</texture>
	</wnd_frame>
	
	<combobox>
		<frame x="0" y="0" width="150" height="21">
			<texture>ui_inv_wnd_frame</texture>
		</frame>
		<btn_arrow_down x="133" y="2" width="15" height="17">
			<texture>ui_inv_elem_combo_button</texture>
		</btn_arrow_down>
		<first_item x="5" y="2" width="120" height="20">
			<text font="letterica16" r="255" g="255" b="255" a="255" align="l" />
		</first_item>
		<list_frame x="0" y="22" width="150" height="204">
			<texture>ui_inv_wnd_frame</texture>
		</list_frame>
		<items_list x="0" y="24" width="149" height="200" item_height="18" always_show_scroll="1" />
	</combobox>
	
	<menu_frame>
		<frame x="0" y="0" width="600" height="30">
			<texture>ui_stroketextbox</texture>
		</frame>
		<btn_population x="5" y="2" width="80" height="30" hint="alife population">
			<text align="l" x="0" y="0" font="letterica16" a="255" r="128" g="128" b="128">population</text>
		</btn_population>
		<btn_launcher x="100" y="2" width="80" height="30" hint="tools launcher">
			<text align="l" x="0" y="0" font="letterica16" a="255" r="128" g="128" b="128">launcher</text>
		</btn_launcher>
		<btn_spawner x="200" y="2" width="80" height="30" hint="spawner">
			<text align="l" x="0" y="0" font="letterica16" a="255" r="128" g="128" b="128">spawner</text>
		</btn_spawner>
	</menu_frame>
	
	<alife_population_stats>
		<list_descr_frame x="160" y="10" width="435" height="20">
			<texture>ui_stroketextbox</texture>
		</list_descr_frame>
		<list_descr x="160" y="10" width="435" height="20">
			<auto_static x="2" y="3" width="150" height="20">
				<text align="l" font="letterica16" a="255" r="238" g="155" b="23">level</text>
			</auto_static>
			<auto_static x="152" y="3" width="125" height="20">
				<text align="l" font="letterica16" a="255" r="238" g="155" b="23">community</text>
			</auto_static>
			<auto_static x="277" y="3" width="100" height="20">
				<text align="l" font="letterica16" a="255" r="238" g="155" b="23">rank</text>
			</auto_static>
			<auto_static x="377" y="3" width="60" height="20">
				<text align="l" font="letterica16" a="255" r="238" g="155" b="23">quantity</text>
			</auto_static>
		</list_descr>
		<list_frame x="160" y="10" width="435" height="350">
			<texture>ui_stroketextbox</texture>
		</list_frame>
		<listview x="160" y="30" width="435" height="330" item_height="16" always_show_scroll="0" />
		<list_item_div x="0" y="16" width="415" height="1" stretch="1">
			<texture x="117" y="141" width="30" height="1">ui\ui_common.dds</texture>
		</list_item_div>
	</alife_population_stats>
	
	<tools_launcher>
		<jump_frame x="5" y="15" width="160" height="80">
			<texture>ui_stroketextbox</texture>
			<auto_static x="10" y="-7" width="100" height="20">
				<text align="l" font="letterica16" a="255" r="238" g="155" b="23">jump to level</text>
			</auto_static>
		</jump_frame>
		<position_frame x="170" y="15" width="228" height="110">
			<texture>ui_stroketextbox</texture>
			<auto_static x="10" y="-7" width="100" height="20">
				<text align="l" font="letterica16" a="255" r="238" g="155" b="23">Actor position</text>
			</auto_static>
		</position_frame>
		<stc_position x="175" y="30" width="150" height="20">
			<text align="l" font="letterica16" a="255" r="255" g="255" b="255" />
		</stc_position>
		<stc_lvid x="175" y="50" width="150" height="20">
			<text align="l" font="letterica16" a="255" r="255" g="255" b="255" />
		</stc_lvid>
		<stc_gvid x="175" y="70" width="150" height="20">
			<text align="l" font="letterica16" a="255" r="255" g="255" b="255" />
		</stc_gvid>
		<vehicle_buy_frame x="400" y="15" width="180" height="80">
			<texture>ui_stroketextbox</texture>
			<auto_static x="10" y="-7" width="100" height="20">
				<text align="l" font="letterica16" a="255" r="238" g="155" b="23">Vehicle buy</text>
			</auto_static>
		</vehicle_buy_frame>
		<combobox_vehicles>
			<frame x="0" y="0" width="170" height="21">
				<texture>ui_inv_wnd_frame</texture>
			</frame>
			<btn_arrow_down x="153" y="2" width="15" height="17">
				<texture>ui_inv_elem_combo_button</texture>
			</btn_arrow_down>
			<first_item x="5" y="2" width="140" height="20">
				<text font="letterica16" r="255" g="255" b="255" a="255" align="l" />
			</first_item>
			<list_frame x="0" y="22" width="170" height="204">
				<texture>ui_inv_wnd_frame</texture>
			</list_frame>
			<items_list x="0" y="24" width="169" height="200" item_height="18" always_show_scroll="1" />
		</combobox_vehicles>
	</tools_launcher>
	
</window>
