	<blowout_hideplace_location>
		<level_map spot="blowout_hideplace_spot" pointer="quest_pointer"/>
		<mini_map spot="blowout_hideplace_mini_spot" pointer="quest_pointer"/>
	</blowout_hideplace_location>
	
	<blowout_hideplace_spot  x="0" y="0" width="25" height="25" stretch="1" alignment="c" xform_anim="map_spot_new_xform" xform_anim_cyclic="1">
		<texture x="0" y="0" width="32" height="32">ui\shelter_icon</texture>
		<!--<texture>ui_hud_icon_sleep</texture>-->
	</blowout_hideplace_spot>
	
	<blowout_hideplace_mini_spot  x="0" y="0" width="15" height="15" stretch="1" alignment="c" xform_anim="map_spot_new_xform" xform_anim_cyclic="0">
