$prefetch 					= 16
class						= Z_ACIDF
hit_impulse_scale			= .1
sound						= zone_mosquito_bald
postprocess					= postprocess_new

artefacts					= 

hit_type					= burn

disable_time				= 5000									;время игнорирования неживого объекта в зоне (-1 если не нужно)
