local sounds_base = stalker_ids.sound_script + 10000
--/ ----------------------------------------------
--/ theme functions
--/ ----------------------------------------------
function id()
	sounds_base = sounds_base + 1
	return sounds_base - 1 --/>
end

function weather_types(avail_types, npc)
	if level.rain_factor() < 0.07 then
		return "weather_good" --/>
	end
	return "weather_bad" --/>
end

function combat_types(avail_types, npc)
	if npc:best_enemy() or npc:best_danger() then
		return "" --/>
	end
	return "state" --/>
end
