local default_prefix = "characters_voice\\"
local character_prefix = ""

function check_prefix(s)
	local flag = string.sub(s, 1, 1)
	return flag, string.sub(s, 3)
end

function select_sound_from_ltx(obj, section, ltx)
	--/[1] проверка по story_id
	local p = tostring( obj:story_id() )
	if p ~= "nil" and ltx:line_exist(section, p) then
		return ltx:r_string(section, p)
	end
	--/[2] проверка по npc_profile
	p = obj:profile_name()
	if p and ltx:line_exist(section, p) then
		return ltx:r_string(section, p)
	end
	--/[3] проверка по character_community
	p = obj:character_community()
	if p and ltx:line_exist(section, p) then
		return ltx:r_string(section, p)
	end
	--/[4] npc sdk name
	p = obj:name()
	if p and ltx:line_exist(section, p) then
		--[[if ltx:line_exist("infos", p) then
			local cond = ltx:r_string("infos", p)
			if has_info(cond) then
				return nil
			end
		end--]]
		return ltx:r_string(section, p)
	end
	--/[5] загрузка дефолта
	if ltx:line_exist(section, "stalker") then
		return ltx:r_string(section, "stalker")
	end
	return nil
end

function load_sound(obj)
	local obj_id = obj:id()
	--/ функция загрузки звуков на основе данных из ltx (snd_ini)
	load_sound_from_ltx = function(obj, field, tbl)
		tbl.max_ids[obj_id] = {}
		if snd_ini:section_exist(field.."_rnd") then
			local s = select_sound_from_ltx(obj, field.."_rnd", snd_ini)
			if s then
				local flag, path = check_prefix(s)
				if flag == "0" then
					character_prefix = obj:sound_prefix()
					obj:sound_prefix(default_prefix)
				end
				tbl.max_ids[obj_id].rnd_max = obj:add_sound(path, nstl, snd_type.talk, sound_priority, sound_mask, tbl.rnd_id)
		--		log("load_sound_from_ltx: obj=[%s] flag=[%s] prefix=[%s] path=[%s] rnd_max=[%d]:(%s)", obj:name(), flag, obj:sound_prefix(), path, tbl.max_ids[obj_id].rnd_max, "rnd") --/#~#
				if flag == "0" then
					obj:sound_prefix(character_prefix)
				end
			end
		end
		if snd_ini:section_exist(field.."_seq") then
			local s = select_sound_from_ltx(obj, field.."_seq", snd_ini)
			if s then
				local flag, path = check_prefix(s)
				if flag == "0" then
					character_prefix = obj:sound_prefix()
					obj:sound_prefix(default_prefix)
				end
	--			log("load_sound_from_ltx: obj=[%s] flag=[%s] prefix=[%s] path=[%s] :(%s)", obj:name(), flag, obj:sound_prefix(), path, "seq") --/#~#
				tbl.max_ids[obj_id].into_max = obj:add_sound(path, nstl, snd_type.talk, sound_priority, sound_mask, tbl.into_id)
				tbl.max_ids[obj_id].into_cycled = true
				if flag == "0" then
					obj:sound_prefix(character_prefix)
				end
			end
		end
		if snd_ini:section_exist(field.."_single") then
			local s = select_sound_from_ltx(obj, field.."_single", snd_ini)
			if s then
				local flag, path = check_prefix(s)
				if flag == "0" then
					character_prefix = obj:sound_prefix()
					obj:sound_prefix(default_prefix)
				end
--				log("load_sound_from_ltx: obj=[%s] flag=[%s] prefix=[%s] path=[%s] :(%s)", obj:name(), flag, obj:sound_prefix(), path, "single") --/#~#
				tbl.max_ids[obj_id].into_max = obj:add_sound(path, nstl, snd_type.talk, sound_priority, sound_mask, tbl.into_id)
				tbl.max_ids[obj_id].into_cycled = false
				if flag == "0" then
					obj:sound_prefix(character_prefix)
				end
			end
		end
	end
	--/<
	load_sound_themes = function(obj, tbl)
		for k,v in pairs(tbl) do
			if v.types then
				load_sound_themes(obj, v.types) --/> рекурсивный вызов
			else
				load_sound_from_ltx(obj, k, v)
			end
		end
	end
	--/<
	load_sound_themes(obj, theme)
	--/ делаем заготовку в db.sound
	db.sound[obj_id] = {}
--	log("load_sound_from_ltx: loaded for obj=[%s]/[%s] prefix=[%s]", obj:name(), obj:section(), obj:sound_prefix(), "") --/#~#
	--table.print(theme, sModule..":Theme|"..obj:name()) --/#~#
end
