--/ Process
--/------------------------------------------------------------------
function loadInfo(npc, char_ini)
	--/ Load from custom data
	if char_ini and char_ini:section_exist("known_info") then
		local iCnt = char_ini:line_count("known_info")
		local result,idx,value = 0,"",""
		for i=0, iCnt-1 do
			result,idx,value = char_ini:r_line("known_info",i,"","")
			this.give_npc_info(npc, idx)
		end
	end
	--/ Load from generator
	local sCommunity = (npc and npc:character_community()) or ""
	local tInfoCommunity = tInfoByCommunity[sCommunity]
	if not tInfoCommunity then
		return --/>
	end
	
	local sRank = ranks.get_obj_rank_name(npc)
	local tInfoRank = tInfoByRank[sRank]
	if not tInfoRank then
		return --/>
	end
	
	local tInfo = {}
	--/ leaving infoportions which are there and there
	for _,v in pairs(tInfoCommunity) do
		for _,vv in pairs(tInfoRank) do
			if vv == v then
				table.insert(tInfo, v) --/ adding to list of avaliable knowledges
				break
			end
		end
	end
	
	local iSize = #tInfo
	if iSize > 1 then
		for i=1,math.min(3,iSize) do
			if math.random() >= 0.6 then
				local sInfo = tInfo[math.random(iSize)]
				this.give_npc_info(npc, sInfo)
			end
		end
	else
		printf("loadInfo:NPC=[%s],Size=[%s]~?:<%s>", npc and npc:name(), iSize, "Warning!") --/#~#
	end
end

--/--------------------------
--/ Giving infoportion to NPC
--/--------------------------
function give_npc_info(npc, sInfo)
--	log("give_npc_info:NPC=[%s],Info=[%s]:[%s]", npc and npc:name(), sInfo, ">") --/#~#
	if npc and sInfo then
--		log("give_npc_info:NPC=[%s],Info=[%s]", npc:name(), sInfo, "") --/#~#
		npc:give_info_portion(sInfo)
		--/ Checking if there is voiced history on that infoportion, and if there is, add it too
		if sound_theme.theme[sInfo] then
			local tStoryStor = db.story_by_id[npc:id()]
			if not tStoryStor then
				tStoryStor = {}
				db.story_by_id[npc:id()] = tStoryStor
			end
			table.insert(tStoryStor, sInfo)
		end
	end
end
