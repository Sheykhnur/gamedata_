class "action_process_info"
--/ ---------------------------------------------
function action_process_info:__init(obj, st)
	self.object = obj
	self.st     = st
	self.log    = st.log
end
--/ -----------------------------------------------------------------------------------------------
--/ BINDER
--/ -----------------------------------------------------------------------------------------------
function add_to_binder(npc, ini, scheme, section, st)
	st.action = this.action_process_info(npc, st)
end
--/ -----------------------------------------------------------------------------------------------
--/ SET SCHEME (moved from stalker_generic.script)
--/ -----------------------------------------------------------------------------------------------
function set_scheme(npc, ini, scheme, section)
--	log("set_npc_info:NPC=[%s],section=[%s]:(%s)", npc:name(), section, ">") --/#~#
	local st = xr_logic.assign_storage_and_bind(npc, ini, scheme, section)
	
	for _,info_section in ipairs("in","out") do
		local str = Get_Cfg_String(ini, section, info_section)
		if str then
			local infolist,condlist = {},{}
			for field in str:gmatch("(%|*[^%|]+%|*)%p*") do
				condlist = field and xr_logic.parse_condlist(npc, info_section, field, field)
				if condlist then
					table.insert(infolist, xr_logic.pick_section_from_condlist(db.actor, npc, condlist))
				end
			end
			for _,v in ipairs(infolist) do
				if info_section == "in" then
					npc:give_info_portion(v)
				else
					npc:disable_info_portion(v)
				end
--				log("set_npc_info:NPC=[%s],info=[%s]:(%s/%s)", npc:name(), v, section, info_section, "") --/#~#
			end
		end
	end
	--/ fot debug:
	if st.log then log("set_scheme: NPC=[%s],scheme=[%s],section=[%s]", npc:name(), scheme, section, "") end --/#~#
end
