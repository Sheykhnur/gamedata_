--/ -----------------------------------------------------------------
--/ VARIABLEs
--/ -----------------------------------------------------------------
local bFixed  = false --/ bool-flag
local bInit   = false
local sModule = script_name() --/ string is the name of the this module file
--/ -----------------------------------------------------------------
--/ Print-Log (output of the debug information)
--/ -----------------------------------------------------------------
local log = function(fmt, ...) _G.log(sModule..":"..tostring(fmt), ...) end
--/ -----------------------------------------------------------------
--/ Initialize Module
--/ -----------------------------------------------------------------
function init()
--	debug_print("init:[>]")
	if bInit then return end --/>
	if event then
		event("presets"):register(process)
	end
end
--/ -----------------------------------------------------------------
--/ Process
--/ -----------------------------------------------------------------
function process(e)
	if bFixed then return end
	--/ -------------------------------------------
	--[[
	--]]
	--/ -------------------------------------------
	bFixed = true
end
